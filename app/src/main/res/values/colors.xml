<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="white">#ffffffff</color>
    <color name="black">#ff000000</color>

    <!-- Primary colors -->
    <color name="colorPrimaryBaseline">#3f6900</color>
    <color name="colorPrimaryVariantBaseline">#6fae19</color>
    <color name="colorSecondaryBaseline">#4b662a</color>
    <color name="colorSecondaryVariantBaseline">#d0f2a5</color>
    <color name="colorTertiaryBaseline">#006c50</color>
    <color name="colorTertiaryVariantBaseline">#24b087</color>

    <!-- Background and surfaces -->
    <color name="colorBackgroundBaseline">#d8dccc</color>
    <color name="colorSurfaceBaseline">#f7fbea</color>

    <!-- Text and icons -->
    <color name="colorOnPrimaryBaseline">#ffffff</color>
    <color name="colorOnPrimaryVariantBaseline">#071200</color>
    <color name="colorOnSecondaryBaseline">#ffffff</color>
    <color name="colorOnSecondaryVariantBaseline">#385217</color>
    <color name="colorOnTertiaryBaseline">#ffffff</color>
    <color name="colorOnTertiaryVariantBaseline">#00110a</color>
    <color name="colorOnBackgroundBaseline">#424937</color>
    <color name="colorOnSurfaceBaseline">#191d13</color>

    <!-- Error colors -->
    <color name="colorErrorBaseline">#ba1a1a</color>
    <color name="colorOnErrorBaseline">#ffffff</color>

    <!-- Widget colors -->
    <color name="colorWidgetText">@color/colorPrimaryBaseline</color>
    <color name="colorWidgetIcon">@color/colorOnTertiaryVariantBaseline</color>
    <color name="colorWidgetIconFill">@color/colorTertiaryVariantBaseline</color>
    <color name="colorWidgetBackground">@color/colorBackgroundBaseline</color>
    <color name="colorWidgetTextOnBackground">@color/colorPrimaryBaseline</color>

    <!-- Overlay colors -->
    <color name="colorOverlayText">@color/colorPrimaryVariantBaseline</color>

    <!-- App UI colors -->
    <color name="colorAppBarIconBaseline">@color/colorPrimaryBaseline</color>
</resources>